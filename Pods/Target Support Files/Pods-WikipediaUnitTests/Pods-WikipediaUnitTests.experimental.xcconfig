OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AFNetworking" -isystem "${PODS_ROOT}/Headers/Public/BlocksKit" -isystem "${PODS_ROOT}/Headers/Public/Colours" -isystem "${PODS_ROOT}/Headers/Public/Expecta" -isystem "${PODS_ROOT}/Headers/Public/HockeySDK" -isystem "${PODS_ROOT}/Headers/Public/LoremIpsum" -isystem "${PODS_ROOT}/Headers/Public/Masonry" -isystem "${PODS_ROOT}/Headers/Public/OCHamcrest" -isystem "${PODS_ROOT}/Headers/Public/OCMockito" -isystem "${PODS_ROOT}/Headers/Public/Tweaks" -isystem "${PODS_ROOT}/Headers/Public/UIImage+AverageColor" -isystem "${PODS_ROOT}/Headers/Public/hpple"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Pods-WikipediaUnitTests-Expecta" -l"Pods-WikipediaUnitTests-OCHamcrest" -l"Pods-WikipediaUnitTests-OCMockito" -framework "Foundation" -framework "XCTest"
